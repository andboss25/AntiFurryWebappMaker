{
    "app-info":{
        "name":"Test app",
        "version":1.0
    },
    "globals":{
    },
    "points":{
        "/":{
            "children":{},
            "type":"Page",
            "name":"Home Page",
            "prop":{
                "source-page":"Pages\\index.html"
            }
        },
        "api":{
            "children":{
                "images":{
                "children":{
                    "antifurry":{
                        "children":{},
                        "type":"StreamBytes",
                        "name":"Image",
                        "prop":{
                            "scriptable":false,
                            "set-response":"Pages\\image.png",
                            "set-response-type":"image/png",
                            "set-response-code":200
                        }
                    }
                },
                "type":"Path",
                "name":"Api Path",
                "prop":{}
                },
                "hello":{
                    "children":{},
                    "type":"Endpoint",
                    "name":"Test Endpoint",
                    "prop":{
                        "scriptable":false,
                        "set-response":"{'Message':'Hello there this is a test!'}",
                        "set-response-type":"application/json",
                        "set-response-code":200
                    }
                },
                "shit":{
                    "children":{},
                    "type":"Endpoint",
                    "name":"Scripted This",
                    "prop":{
                        "scriptable":true,
                        "script-path":"Scripts\\test.lua"
                    }
                }
            },
            
            "type":"Path",
            "name":"Api Path",
            "prop":{
            }
        }
    },
    "special-points":{
        "hascode 404;":{
            "children":{},
            "type":"Endpoint",
            "name":"Code 404",
            "prop":{
                "scriptable":false,
                "set-response":"{'Message':'Page not found!'}",
                "set-response-type":"application/json",
                "set-response-code":404
            }
        }
    }
}